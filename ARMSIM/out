    %load/v 8, v008DEED0_0, 4;
    %set/v v008DEF80_0, 8, 4;
    %jmp T_0.4;
T_0.4 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_008D4150;
T_1 ;
    %delay 1000, 0;
    %vpi_call 2 10 "$finish";
    %end;
    .thread T_1;
    .scope S_008D4150;
T_2 ;
    %movi 8, 5, 4;
    %set/v v008D1668_0, 8, 4;
    %set/v v008D16C0_0, 1, 4;
    %set/v v008D1718_0, 0, 4;
    %movi 8, 10, 4;
    %set/v v008D1770_0, 8, 4;
    %set/v v008D8538_0, 0, 2;
