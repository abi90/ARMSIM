#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_001C3A78 .scope module, "test_mux_2x1" "test_mux_2x1" 2 1;
 .timescale 0 0;
P_00F92414 .param/l "sim_time" 2 5, +C4<01100100>;
v00F93168_0 .var "I0", 31 0;
v00F931C0_0 .var "S", 0 0;
v001CE1F0_0 .net "Y", 4 0, v00F93110_0; 1 drivers
S_001C2A88 .scope module, "sel" "amount_selector" 2 6, 3 1, S_001C3A78;
 .timescale 0 0;
v001C2B10_0 .net "IR", 31 0, v00F93168_0; 1 drivers
v001C2B68_0 .net "S", 0 0, v00F931C0_0; 1 drivers
v00F93110_0 .var "Y", 4 0;
E_00F92270 .event edge, v001C2B10_0, v001C2B68_0;
    .scope S_001C2A88;
T_0 ;
    %wait E_00F92270;
    %load/v 8, v001C2B68_0, 1;
    %jmp/0xz  T_0.0, 8;
    %ix/load 1, 7, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.2, 4;
    %load/x1p 8, v001C2B10_0, 5;
    %jmp T_0.3;
T_0.2 ;
    %mov 8, 2, 5;
T_0.3 ;
; Save base=8 wid=5 in lookaside.
    %set/v v00F93110_0, 8, 5;
    %jmp T_0.1;
T_0.0 ;
    %mov 8, 0, 1;
    %ix/load 1, 8, 0;
    %mov 4, 0, 1;
    %jmp/1 T_0.4, 4;
    %load/x1p 13, v001C2B10_0, 4;
    %jmp T_0.5;
T_0.4 ;
    %mov 13, 2, 4;
T_0.5 ;
    %mov 9, 13, 4; Move signal select into place
    %set/v v00F93110_0, 8, 5;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_001C3A78;
T_1 ;
    %delay 100, 0;
    %vpi_call 2 7 "$finish";
    %end;
    .thread T_1;
    .scope S_001C3A78;
T_2 ;
    %set/v v00F93168_0, 1, 32;
    %set/v v00F931C0_0, 0, 1;
    %delay 10, 0;
    %ix/load 0, 1, 0;
    %load/vp0 8, v00F931C0_0, 1;
    %set/v v00F931C0_0, 8, 1;
    %end;
    .thread T_2;
    .scope S_001C3A78;
T_3 ;
    %vpi_call 2 14 "$display", " S I I1 I0 Y";
    %vpi_call 2 15 "$monitor", " %b %b %b", v00F931C0_0, v00F93168_0, v001CE1F0_0;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
