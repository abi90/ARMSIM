T_2.0 %cmp/s 0, 8, 3;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 3;
    %delay 10, 0;
    %ix/load 0, 1, 0;
    %load/vp0 11, v008D8538_0, 2;
    %set/v v008D8538_0, 11, 2;
