    .scope S_008D9738;
T_0 ;
    %wait E_00793508;
    %load/v 8, v008DEF28_0, 2;
    %cmpi/u 8, 0, 2;
    %jmp/1 T_0.0, 6;
    %cmpi/u 8, 1, 2;
    %jmp/1 T_0.1, 6;
    %cmpi/u 8, 2, 2;
    %jmp/1 T_0.2, 6;
    %cmpi/u 8, 3, 2;
    %jmp/1 T_0.3, 6;
    %jmp T_0.4;
T_0.0 ;
    %load/v 8, v00793F90_0, 4;
    %set/v v008DEF80_0, 8, 4;
    %jmp T_0.4;
T_0.1 ;
